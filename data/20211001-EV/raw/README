zcat *R1_001.fastq.gz | paste - - - - | cut -f2 | grep ATCTCGTATGCCGTCTTCTGCTTG > ATCTCGTATGCCGTCTTCTGCTTG.reads.txt


BOX="https://dav.box.com/dav/Francis _Lab_Share/20211001-EV"
