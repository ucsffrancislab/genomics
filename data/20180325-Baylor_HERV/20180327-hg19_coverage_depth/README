for f in $( ls *R2_001.fastq.gz ); do
m=${f%2_001.fastq.gz}
n=${f%_??????_L001_R2_001.fastq.gz}
echo $f $m $n
bowtie2 --very-sensitive -x hg19 -1 ${m}1_001.fastq.gz -2 $f --threads 4 | samtools sort -o ${n}.hg19.bam -
samtools index ${n}.hg19.bam
done


