for f in $( ls *.hg19.bam ); do
n=${f%.hg19.bam}
echo $f $n
samtools depth -a $f | awk -F"\t" '( $3 > 1000 ){c=$1;p2=$2;if(p1=="")p1=$2;} ( $3 <= 1000 && c != "" ){ print c" : "p1"-"p2; c=p1=p2="";}' > ${n}.hg19.gt1000.txt
samtools depth -a $f | awk -F"\t" '( $3 > 5000 ){c=$1;p2=$2;if(p1=="")p1=$2;} ( $3 <= 5000 && c != "" ){ print c" : "p1"-"p2; c=p1=p2="";}' > ${n}.hg19.gt5000.txt
done

The -a option really takes long
It would be nice to add some better logic to check contiguouity.

for f in $( ls *.hg19.bam ); do
n=${f%.hg19.bam}
echo $f $n
samtools depth $f | awk -F"\t" '( $3 > 1000 ){c=$1;p2=$2;if(p1=="")p1=$2;next;}( ( $3 <= 1000 && c != "" ) || ( $3 != p1+1 && c != "" ) ){ print c" : "p1"-"p2; c=p1=p2="";}' > ${n}.hg19.gt1000.txt
samtools depth $f | awk -F"\t" '( $3 > 5000 ){c=$1;p2=$2;if(p1=="")p1=$2;next;}( ( $3 <= 5000 && c != "" ) || ( $3 != p1+1 && c != "" ) ){ print c" : "p1"-"p2; c=p1=p2="";}' > ${n}.hg19.gt5000.txt
done




for f in $( ls *.hg19.bam ); do
n=${f%.hg19.bam}
echo $f $n
samtools depth $f | awk -F"\t" '( $3 > 500 ){c=$1;p2=$2;if(p1=="")p1=$2;next;}( ( $3 <= 500 && c != "" ) || ( $3 != p1+1 && c != "" ) ){ print c" : "p1"-"p2; c=p1=p2="";}' > ${n}.hg19.gt500.txt
done







