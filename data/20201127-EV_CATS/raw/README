curl -netrc -T ${f} "${BOX}/"
done


bowtie2.bash --very-sensitive -x human_mirna -U $f -o ${base}.vs_mirna.bam

for f in output/trimmed*q.gz ; do
base=${f%_001.fastq.gz}
base=${base/_S?_L001/}
echo $f
bowtie2.bash --very-sensitive-local -x human_mirna -U $f -o ${base}.vsl_mirna.bam
done


