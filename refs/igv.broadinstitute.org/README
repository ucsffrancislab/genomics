for rmask in annotations/hg19/variations/*.rmask.gz ; do
echo $rmask
gtf=${rmask/rmask.gz/gtf}
zcat ${rmask} | awk 'BEGIN{FS=OFS="\t"}{i[$11]++;print $6,"source","feature",1+$7,$8,".",$10,".","feature_name \""$11"."i[$11]"\""}' > ${gtf}
done



